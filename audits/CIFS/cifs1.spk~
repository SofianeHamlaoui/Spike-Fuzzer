    packet+=binstring("FF 53 4D 42") #SMB
                        packet+=binstring("72") #SMB Command: Negotiate Protocol
                        packet+=binstring("00") #Error Class: Success
                        packet+=binstring("00") #reserved
                        packet+=binstring("00 00") #error code: no error
                        packet+=binstring("00") #flags (none)
                        packet+=binstring("00 00") #flags2 (none)
                        packet+=binstring("00 00") #Process ID High (0)
                        packet+=binstring("00 00 00 00 00 00 00 00") #signature
                        packet+=binstring("00 00") #Reserved                        
                        packet+=binstring("00 00") #Tree ID                        
                        packet+=binstring("00 00") #Process ID 
                        packet+=binstring("00 00") #User ID 
                        packet+=binstring("00 00") #Multiplex ID 
                        packet+=binstring("00") #word count
                        dialects="\x02"+"NT LM 0.12"+"\x00"
                        bytecount=len(dialects)
                        packet+=halfword2istr(bytecount)
                        packet+=dialects
                        packet=netbios(packet)
